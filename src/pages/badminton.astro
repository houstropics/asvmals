---
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import Title from '../components/Title.astro';
import Gallery from '../components/Gallery.astro';
import SektionBanner from '../components/SektionBanner.astro';
import SektionInfo from '../components/SektionInfo.astro';
import badmintonPic from '../images/index/badminton.svg';

import Banner from '../images/badminton/Banner.jpg';
import image1 from '../images/badminton/image1.jpg';
import image2 from '../images/badminton/image2.jpg';
import image3 from '../images/badminton/image3.jpg';
import image4 from '../images/badminton/image4.jpg';
import image5 from '../images/badminton/image5.jpg';
import image6 from '../images/badminton/image6.jpg';
import image7 from '../images/badminton/image7.jpg';
import image8 from '../images/badminton/image8.jpg';
import image9 from '../images/badminton/image9.jpg';

import raffiPic from '../images/fussball/raffi.jpg';
import raffiLg from '../images/fussball/raffi_lg.jpg';
import preGameLg from '../images/fussball/pregameLg.jpg';
import preGamesd from '../images/fussball/pregame_sd.jpg';
import MannschaftPic from '../images/fussball/Mannschaft.jpg';
---

<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />
    <title>ASV Mals</title>
    <style is:global>
        @import '../styles/global.css'; 
      </style>
    <link rel="preload" href="/fonts/univermag.ttf" as="font" type="font/ttf" crossorigin>
    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
</head>

  <Header></Header>

  
	<SektionBanner
	Sektion="Badminton"
	Banner={Banner.src}
	Icon={badmintonPic.src}
	/>


	<SektionInfo 
	Sektionsleiter="Stefan De March" 
	Telefon="+39 335 5855123" 
	Email="malles@badmintonitalia.net"
	Insta="https://www.instagram.com/asv_mals_badminton/"
	Facebook="https://www.facebook.com/ASVMalsBadminton/" 
	Web="http://www.badmintonmals.it/"
	Webadress="www.badmintonmals.it"
	Image1Big={raffiLg.src} 
	Image1Small={raffiPic.src} 
	Image2Big={preGameLg.src}
	Image2Small={preGamesd.src}
	Image1Alt="Raffael"
	Image2Alt="Pregame"
	/>

	
  	<Title title="Fotogalerie"/>

	<Gallery 
	image1={image1.src} image1Alt="badminton"
	image2={image2.src} image2Alt="badminton"
	image3={image8.src} image3Alt="badminton"
	image4={image4.src} image4Alt="badminton"
	image5={image5.src} image5Alt="badminton"
	image6={image6.src} image6Alt="badminton"
	image7={image7.src} image7Alt="badminton"
	image8={image3.src} image8Alt="badminton"
	image9={image9.src} image9Alt="badminton"
	/>

	
  <Footer></Footer>

</body>
</html>
