---
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import SektionBanner from '../components/SektionBanner.astro';
import SektionShort from '../components/SektionShort.astro';
import sportplatzPic from '../images/index/sportplatz.webp';

import bogenschützenPic from '../images/index/bogenschützen.svg';
import bogenschuetzen from '../images/bogenschuetzen/bogenschuetzen.jpg';


---

<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />
    <title>ASV Mals</title>
    <style is:global>
        @import '../styles/global.css'; 
      </style>
    <link rel="preload" href="/fonts/univermag.ttf" as="font" type="font/ttf" crossorigin>
    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
</head>

  <Header></Header>

  
	<SektionBanner
	Sektion="Bogenschuetzen"
	Banner={bogenschuetzen.src}
	BannerMobile={bogenschuetzen.src}
	Icon={bogenschützenPic.src}
	/>


	<SektionShort
	Sektionsleiter="René Guizzo" 
	Telefon="+39 340 6386843"
	Emailyes="Email"
	Email="guizzo.rene94@gmail.com" 
	Image1Big={sportplatzPic.src} 
	Image1Small={sportplatzPic.src} 
	Image1Alt="sportplatz"
	/>

	
  <Footer></Footer>

</body>
</html>
